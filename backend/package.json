{
  "name": "growgrow-backend",
  "version": "1.0.0",
  "description": "Backend for the GROW YouR NEED Super App.",
  "main": "dist/server.js",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.9.2",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "local:prod": "pnpm build && node dist/server.js",
  "test": "vitest run --globals",
    "test:coverage": "vitest run --coverage --globals",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "validate:env": "dotenv-safe --example .env.example",
    "prepare": "echo 'skip prepare during workspace install'",
    "clean": "rm -rf dist node_modules .turbo .pnpm-store",
    "migrate": "pnpm prisma migrate dev",
    "generate": "pnpm prisma generate",
    "seed": "pnpm ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@google/genai": "^0.12.0",
    "@prisma/client": "^5.15.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.14",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "tsx": "^4.7.0",
    "dotenv-safe": "^8.2.0"
  }
}
